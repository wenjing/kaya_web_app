<header id="signedin_banner">
  <div class="clearfix container">
    <nav>
    <ul class="jsddm clearfix">
      <li><%#= link_to "Home", current_user %></li>
      <li><%= link_to "Meets", meets_user_path(current_user) %><ul>
        <li><%= link_to "Solo", meets_user_path_of_type(current_user, 1) %></li>
        <li><%= link_to "Private", meets_user_path_of_type(current_user, 2) %></li>
        <li><%= link_to "Group", meets_user_path_of_type(current_user, 3) %></li>
      </ul></li>
      <li><%= link_to "Friends", friends_user_path(current_user) %></li>
      <li><%#= link_to "Comments", comments_user_path(current_user) %></li>
