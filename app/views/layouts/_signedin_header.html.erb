        <li><%= link_to "Solo", map_user_path_of_type(current_user, 1) %></li>
        <li><%= link_to "Private", map_user_path_of_type(current_user, 2) %></li>
        <li><%= link_to "Group", map_user_path_of_type(current_user, 3) %></li>
      </ul></li>
      <li no_border id="user_profile_title">
